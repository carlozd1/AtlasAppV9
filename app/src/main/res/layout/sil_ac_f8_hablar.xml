    <ImageButton
        android:layout_height="150dp"
        android:id="@+id/btn_hablar_ha"
        android:background="@drawable/ac_ha"
        android:layout_width="140dp"
        android:layout_marginLeft="78dp"
        android:layout_marginStart="78dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/obtn_Hablar"
        android:layout_alignStart="@+id/obtn_Hablar" />

</RelativeLayout>