<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.carlos.atlasapp.Nivel2.nivel2_botones1_fonemas">

    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="A"
        android:textSize="48dp"
        android:id="@+id/textView4"
        android:padding="10dp"/>

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Abrigo"
        android:id="@+id/btnAbrigo"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Aguja"
        android:id="@+id/btnAguja"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/btnAbrigo"
        android:layout_toEndOf="@+id/btnAbrigo" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Araña"
        android:id="@+id/btnAraña"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/btnAguja"
        android:layout_toEndOf="@+id/btnAguja" />

    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="B"
        android:textSize="48dp"
        android:id="@+id/textView5"
        android:padding="10dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Barco"
        android:id="@+id/btnBarco"
        android:layout_below="@+id/textView4"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Bici"
        android:id="@+id/btnBici"
        android:layout_alignBottom="@+id/btnBarco"
        android:layout_toRightOf="@+id/btnAbrigo"
        android:layout_toEndOf="@+id/btnAbrigo"
        android:layout_below="@+id/btnAbrigo" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Bota"
        android:id="@+id/btnBota"
        android:layout_below="@+id/btnAraña"
        android:layout_toRightOf="@+id/btnAguja"
        android:layout_toEndOf="@+id/btnAguja" />

    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="C"
        android:textSize="48dp"
        android:id="@+id/textView6"
        android:padding="10dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Cama"
        android:id="@+id/btnCama"
        android:layout_below="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Comida"
        android:id="@+id/btnComida"
        android:layout_below="@+id/btnBici"
        android:layout_toRightOf="@+id/btnCama"
        android:layout_toEndOf="@+id/btnCama" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Casa"
        android:id="@+id/btnCasa"
        android:layout_below="@+id/btnBota"
        android:layout_toRightOf="@+id/btnBici"
        android:layout_toEndOf="@+id/btnBici" />

    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="E"
        android:textSize="48dp"
        android:id="@+id/textView7"
        android:padding="10dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="G"
        android:textSize="48dp"
        android:id="@+id/textView8"
        android:padding="10dp"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="J"
        android:textSize="48dp"
        android:id="@+id/textView9"
        android:padding="10dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="L"
        android:textSize="48dp"
        android:id="@+id/textView10"
        android:padding="10dp"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Escoba"
        android:id="@+id/btnEscoba"
        android:layout_below="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Espejo"
        android:id="@+id/btnEspejo"
        android:layout_below="@+id/btnComida"
        android:layout_toRightOf="@+id/btnCama"
        android:layout_toEndOf="@+id/btnCama" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Gallina"
        android:id="@+id/btnGallina"
        android:layout_below="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Gancho"
        android:id="@+id/btnGancho"
        android:layout_below="@+id/btnEspejo"
        android:layout_toRightOf="@+id/btnEscoba"
        android:layout_toEndOf="@+id/btnEscoba" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Espuma"
        android:id="@+id/btnEspuma"
        android:layout_below="@+id/btnCasa"
        android:layout_toRightOf="@+id/btnComida"
        android:layout_toEndOf="@+id/btnComida" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Goma"
        android:id="@+id/btnGoma"
        android:layout_below="@+id/btnEspuma"
        android:layout_toRightOf="@+id/btnGancho"
        android:layout_toEndOf="@+id/btnGancho" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Jabón"
        android:id="@+id/btnJabon"
        android:layout_below="@+id/textView8"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Jarra"
        android:id="@+id/btnJarra"
        android:layout_below="@+id/btnGancho"
        android:layout_toRightOf="@+id/btnGallina"
        android:layout_toEndOf="@+id/btnGallina" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Jovén"
        android:id="@+id/btnJoven"
        android:layout_below="@+id/btnGoma"
        android:layout_toRightOf="@+id/btnGancho"
        android:layout_toEndOf="@+id/btnGancho" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Libro"
        android:id="@+id/btnLibro"
        android:layout_below="@+id/textView9"
        android:layout_toRightOf="@+id/textView9"
        android:layout_toEndOf="@+id/textView9" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Lentes"
        android:id="@+id/btnLentes"
        android:layout_below="@+id/btnJarra"
        android:layout_toRightOf="@+id/btnJabon"
        android:layout_toEndOf="@+id/btnJabon" />

    <Button
        android:layout_width="175dp"
        android:layout_height="75dp"
        android:text="Lápiz"
        android:id="@+id/btnLapiz"
        android:layout_below="@+id/btnJoven"
        android:layout_toRightOf="@+id/btnJarra"
        android:layout_toEndOf="@+id/btnJarra" />


</RelativeLayout>
